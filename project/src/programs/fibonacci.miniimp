def main with input in output out as
    x := in;
    out := 0;
    if not x < 2 then (
        out := 1;
        second := 0;
        while not x < 2 do (
            temp := out;
            out := out + second;
            second := temp;
            x := x - 1
        )
    )
    else (
        out := 1
    )
